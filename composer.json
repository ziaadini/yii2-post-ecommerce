{
  "name": "ziaadini/yii2-post-ecommerce",
  "description": "Electronic Commerce Service of National Iranian Post Company For Yii2 Framework",
  "keywords": [
    "yii2",
    "framework",
    "Post Ecommerce Service"
  ],
  "homepage": "https://github.com/ziaadini/yii2-post-ecommerce",
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/ziaadini/yii2-post-ecommerce/issues?state=open",
    "source": "https://github.com/ziaadini/yii2-post-ecommerce"
  },
  "authors": [
    {
      "name": "Sadegh Shafii",
      "email": "sadshafiei.01@gmail.com",
      "homepage": "https://github.com/Sadi01"
    },
    {
      "name": "Yadollah Ziaadini",
      "email": "y.ziaadini@gmail.com",
      "homepage": "https://github.com/ziaadini"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "yiisoft/yii2": "~2.0.14",
    "filsh/yii2-oauth2-server": "^2.0",
    "yiisoft/yii2-httpclient": "~2.0.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": { "ziaadini\\postecommerce\\": "src" }
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "extra": {
    "bootstrap": "sadi01\\postecommerce\\Bootstrap"
  }
}
